pll() {
git clone https://github.com/rooted-cyber/install-package
cp ins*pac*/pl*/* plug*
pwd
pwd
ls ins*pac*
cp ins*pac*/req* /root/ultroid2/Ultroid/Ultroid/a.txt
git clone https://github.com/rooted-cyber/install-package;cp ins*pac*/pl*/* plug*
}
inp() {
sudo apt --fix-broken install -y
apt install python3 python3-pip -y
sudo apt install python3 python3-pip -y
apt --fix-broken install -y
pip install --upgrade pip
pip3 install -r req*
pip3 install -r a.txt
pip3 install -r res*/st*/op*
apt --fix-broken install -y
pip install -U yt-dlp

}
ult() {
git clone https://github.com/TeamUltroid/Ultroid
sudo chmod 777 -R *
cd Ultro*
pll
envv
inp > /dev/null 2>&1
bash startup
}
